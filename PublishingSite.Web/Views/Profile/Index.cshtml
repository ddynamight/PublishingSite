@model PublishingSite.Model.AppUser



@{
     ViewData["Title"] = "Profile";
}



<div id="innerCon">
     <div class="container-fluid">
          <h2 class="text-center">My Profile</h2>

          <form asp-controller="Profile" asp-action="Create" method="post">
               <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group">
                         <label for="titleDdl">Title</label>
                         <h4>@Model.Title</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbFirstname">Firstname</label>
                         <h4>@Model.Firstname</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbMiddlename">Middlename</label>
                         <h4>@Model.Middlename</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbLastname">Lastname</label>
                         <h4>@Model.Lastname</h4>
                    </div>

                    <div class="form-group">
                         <label for="sexDdl">Sex</label>
                         <h4>@Model.Sex</h4>
                    </div>
               </div>

               <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group">
                         <label for="countryDdl">Country</label>
                         <h4>@Model.Country</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbUniversity">University</label>
                         <h4>@Model.University</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbPostCode">Post Code</label>
                         <h4>@Model.PostCode</h4>
                    </div>

                    <div class="form-group">
                         <label for="tbPhone">Phone Number</label>
                         <h4>@Model.PhoneNumber</h4>
                    </div>
               </div>

               <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
                    <a href="/Profile/Edit">Edit Profile</a>
               </div>
          </form>
     </div>
</div>
